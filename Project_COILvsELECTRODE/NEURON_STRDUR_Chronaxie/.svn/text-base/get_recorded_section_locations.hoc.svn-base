/* Computes xyz coords of nodes in a model cell
 whose topology & geometry are defined by pt3d data.
 Expects sections to already exist, and that the xtra mechanism has been inserted
 */
 
objref sh // #1 *****
sh = new PlotShape() // #2 *****
sh.size(-300,300,-300,300) // #3 *****
 
objref tmps,tmplist
tmps = new SectionList()
tmplist = new List()
tmps.wholetree()
numsecs = 0
forsec tmps numsecs = numsecs+1

objref xrec, yrec, zrec, pfout, pfsecs

pfout=new File()
pfsecs = new File()
pfout.wopen("./VoutLocations_ax20mm.txt")
pfsecs.wopen("./VoutSections_ax20mm.txt")

xrec = new Vector(numsecs)
yrec = new Vector(numsecs)
zrec = new Vector(numsecs)

i=0
forsec tmps {
	xrec.x[i] = x3d(.5)
	yrec.x[i] = y3d(.5)
	zrec.x[i] = z3d(.5)
	
	print i
	print secname()
	i=i+1
	pfsecs.printf("%s\n",secname())
}
pfsecs.close()

xrec.printf(pfout,"%.4f\n")
yrec.printf(pfout,"%.4f\n")
zrec.printf(pfout,"%.4f\n")
pfout.close()